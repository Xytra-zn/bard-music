{
  "expo": {
    "name": "Music Bot",
    "slug": "your-bot-slug",
    "privacy": "public",
    "sdkVersion": "47.0.0",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "version": "1.0.0",
    "description": "Music bot that plays songs from YouTube and Spotify.",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*.png",
      "**/*.jpg",
      "**/*.js"
    ]
  },
  "credentials": {
    "API_ID": "your-api-id",
    "API_HASH": "your-api-hash",
    "BOT_TOKEN": "your-bot-token"
  }
}
